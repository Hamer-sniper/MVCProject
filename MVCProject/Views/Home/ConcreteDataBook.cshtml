@model DataBook
@{
    ViewData["Title"] = "MyMVCProject";
}

<div class="text-center" align="center">
    <h1 class="display-4">Записная книжка</h1>
</div>

<div align="center">
    <table border="1">
        <tr>
            <th>ID</th>
            <th>Фамилия</th>
            <th>Имя</th>
            <th>Отчество</th>
            <th>Номер телефона</th>
            <th>Адрес</th>
            <th>Описание</th>
        </tr>
        <tr>
            <td>@Model.ID.ToString()</td>
            <td>@Model.Surname</td>
            <td>@Model.Name</td>
            <td>@Model.MiddleName</td><
            <td>@Model.TelephoneNumber</td>
            <td>@Model.Adress</td>
            <td>@Model.Note</td>
        </tr>
    </table>
</div>

<div align="center">
    <a class="flex-sm-fill text-sm-center nav-link" asp-controller="Home" asp-action="Index">На главную</a>
</div>
